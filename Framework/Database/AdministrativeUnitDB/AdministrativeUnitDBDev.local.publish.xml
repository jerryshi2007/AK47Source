<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>MCS_ADMINISTRATIVE_UNIT</TargetDatabaseName>
    <DeployScriptFileName>AdministrativeUnitDB_Dev.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=MCSFramework;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
