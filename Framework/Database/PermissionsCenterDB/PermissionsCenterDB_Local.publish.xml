<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>MCS_PERMISSIONS_CENTER</TargetDatabaseName>
    <DeployScriptFileName>PermessionsCenterDB.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=MCSFramework;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <DropObjectsNotInSource>True</DropObjectsNotInSource>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <CreateNewDatabase>False</CreateNewDatabase>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="TaskJobName">
      <Value>sc_execute_local_job_list</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>