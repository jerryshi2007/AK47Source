      <add name="EmptyCategoryProperties">
        <properties>
          <add name="Name" description="名称" type="String">
            <validators>
              <add name="StringEmptyValidator" type="MCS.Library.Validation.StringEmptyValidator, MCS.Library" messageTemplate="名称不能为空啊"/>
            </validators>
          </add>
          <add name="MinLength" description="最小长度" type="Integer" defaultValue="0">
            <validators>
              <add name="IntegerRangeValidator" type="MCS.Library.Validation.IntegerRangeValidator, MCS.Library" messageTemplate="最小长度必须在0和10之间">
                <parameters>
                  <add name="lowerBound" type="Integer" paramValue="0"/>
                  <add name="upperBound" type="Integer" paramValue="10"/>
                </parameters>
              </add>
            </validators>
          </add>
          <add name="MaxLength" description="最大长度" type="Integer" defaultValue="100">
            <validators>
              <add name="IntegerRangeValidator" type="MCS.Library.Validation.IntegerRangeValidator, MCS.Library" messageTemplate="最大长度必须在255和300之间">
                <parameters>
                  <add name="lowerBound" type="Integer" paramValue="255"/>
                  <add name="upperBound" type="Integer" paramValue="300"/>
                </parameters>
              </add>
            </validators>
          </add>
          <add name="IsDefaultValue" description="是否默认" category="下拉" type="Boolean" defaultValue="false"/>
          <add name="EnumValue" description="枚" category="下拉" type="Enum" editorKey="EnumPropertyEditor" editorParams="EnumValue"/>
          <add name="PredefinedEnumValue" description="预定义枚举" category="下拉" type="Enum" editorKey="EnumPropertyEditor" editorParams="MCS.Library.SOA.Web.WebControls.Test.PropertyGrid.EnumValueDefine, MCS.Library.SOA.Web.WebControls.Test"/>
