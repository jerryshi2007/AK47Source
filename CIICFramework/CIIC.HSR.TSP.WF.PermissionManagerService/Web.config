  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <!--为配置变量赋值value表示日志的路径-->
    <variable name="logDirectory" value="D:\Logs\" />
    <!--定义日志的目标/输出-->
    <targets>
      <!--<default-wrapper xsi:type="BufferingWrapper" bufferSize="100"/>-->
      <target name="Rolling" xsi:type="File" fileName="${logDirectory}/Rolling${date:format=yyyyMMddHH}.txt" layout="Date:${longdate}${newline}Level:${level:uppercase=true}${newline}Message:${message}${newline}" archiveAboveSize="5000000" Encoding="utf-8" />
      <target name="EFSqlTrace$" xsi:type="File" layout="${longdate}:${message} ${exception:format=message,stacktrace:separator=*}" fileName="${logDirectory}\EFSqlTrace${event-context:DirOrPrefix}${date:format=yyyyMMddHH}${event-context:Suffix}.txt" archiveAboveSize="5000000" Encoding="utf-8" />
    </targets>
    <!--路由规则，表示每个级别的日志所对应的target目标路径，writeTo 对应target中的name-->
    <rules>
      <logger name="Default" minlevel="Trace" writeTo="Rolling" />
      <logger name="EFSqlTract" minlevel="Trace" writeTo="EFSqlTrace$" />
    </rules>
  </nlog>
  <dataCacheClient>
    <hosts>
      <host name="172.16.8.83" cachePort="22233" />
    </hosts>
    <!--isEnabled本地缓存已启用,sync:TimeoutBased 值用于表明应使用超时值。NotificationBased 用于表明还应使用缓存通知。
    ttlValue:本地缓存超时（秒）。
 -->
    <localCache isEnabled="true" sync="NotificationBased" objectCount="100000" ttlValue="300" />
    <!--特定缓存通知轮询时间间隔(秒);最大队列长度,maxQueueLength 属性。如果未指定，则默认值为 10000。-->
    <clientNotification pollInterval="300" />
    <!--注意：mode与protectionLevel要跟server保持一致。开发阶段都设置为None。如果mode=Transport则需要给宿主运行账号授权。
      具体可访问：http://msdn.microsoft.com/zh-cn/library/ff921012(v=azure.10).aspx
      -->
    <securityProperties mode="None" protectionLevel="None" />
    <!--连接缓冲区大小(字节);最大缓冲区池大小(字节);最大缓冲区大小(字节);最大输出延迟(毫秒);通道初始化超时(毫秒);接收超时(毫秒)-->
    <transportProperties connectionBufferSize="131072" maxBufferPoolSize="268435456" maxBufferSize="8388608" maxOutputDelay="2" channelInitializationTimeout="60000" receiveTimeout="600000" />
  </dataCacheClient>
